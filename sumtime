#!/ryougasan21/bin/env python3

import sys
import re

def main():
    content = sys.stdin.read()
    hours = re.findall(r'(\d+(?:\.\d+)?)\s*h', content)
    minutes = re.findall(r'(\d+)\s*m', content)
    total_min = 0
    for h in hours:
        total_min += float(h) * 60
    for m in minutes:
        total_min += int(m)
    display_h = int(total_min // 60)
    display_m = int(total_min % 60)
    if display_h > 0:
        print(f"{display_h}h {display_m}m")
    else:
        print(f"{display_m}m")

if __name__ == "__main__":
    main()
